package com.revature.project0;

public class Offers {
	
	private String customer;
	private double offer;
	private int serial;
	
	public Offers(String customer, double offer, int serial) {
		this.customer = customer;
		this.offer = offer;
		this.serial =serial;
		
	}

	public String getCustomer() {
		return customer;
	}

	public double getOffer() {
		return offer;
	}

	public int getSerial() {
		return serial;
	}

	
}
